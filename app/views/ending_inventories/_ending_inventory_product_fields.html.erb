<div class="nested-fields">
    <div class="fields">
        <div class="field">
            <%= f.association :product, collection: @store.products.where('store_products.inventory > 0') %>
        </div>
        <div class="field">
            <%= f.input :ending_quantity %>
        </div>
         <%= link_to_remove_association 'X', f, style: 'margin-top:2em;color:#7a828a;' %>
    </div>
</div>